unit URegraCRUDCadPessoa;

interface

uses
    URegraCRUD
  , URepositorioDB
  , URepositorioCadPessoa
  , UEntidade
  ;

type
  TRegraCRUDCadPessoa = class(TRegraCRUD)
  public
    constructor Create; override;
  end;

implementation

{ TRegraCRUDCadPessoa }

constructor TRegraCRUDCadPessoa.Create;
begin
  inherited;
  FRepositorioDB := TRepositorioDB<TENTIDADE>(TRepositorioCadPessoa.Create);
end;

end.
