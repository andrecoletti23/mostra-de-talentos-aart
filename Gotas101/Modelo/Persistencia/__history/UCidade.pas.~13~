unit UCidade;

interface

uses
    UEntidade
  ;

type
  TCIDADE = class(TENTIDADE)
  private
    FNome: String;

  public
    function RetornaDescricaoEntidade: String; override;

    property NOME: String read FNome write FNome;
  end;

const
  TBL_CIDADE      = 'CIDADE';
  FLD_CIDADE_ID   = 'ID';
  FLD_CIDADE_NOME = 'NOME';

implementation

{ TCIDADE }

function TCIDADE.RetornaDescricaoEntidade: String;
begin
  Result := 'Cidade';
end;

end.
